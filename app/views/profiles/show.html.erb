<div class='container'>
	<br>
	<h4>My Fishtagrams</h4>
	<% @fishtagram.each do |fishtagram| %>
	  <div class="row">
	    <div class="col s12">
	      <div class="card blue-grey darken-1">
	        <div class="card-content white-text">
	          <span class="card-title"><%=fishtagram.title%></span>
	          <p><%=fishtagram.body%></p>
	        </div>
	      </div>
	    </div>
	  </div>
		<% end %>
	<h4>My fishes</h4>
		<% @fish.each do |fish| %>
		<div class="row">
		  <div class="col s12 m3">
		    <div class="card-panel blue-grey darken-1">
		      <span class="white-text"><%=fish.name%>
		      </span>
		    </div>
		  </div>
		</div>
		<% end %>
	<h4>My following!</h4>
		<% @follow.each do |follow| %>
			<p><%=follow.user.name%></p>
		<% end %>
		<h4>Who is following me!!</h4>
			<% @following.each do |following| %>
				<p><%=following.user.name%></p>
			<% end %>
</div>