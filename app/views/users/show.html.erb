<div class='container'>
<br>
<h4>My bubbles</h4>
<% @bubble.each do |bubble| %>
  <div class="row">
    <div class="col s12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%=bubble.title%></span>
          <p><%=bubble.body%></p>
        </div>
      </div>
    </div>
  </div>
	<% end %>
<h4>My fishes</h4>
	<% @fish.each do |fish| %>
	<div class="row">
	  <div class="col s12 m3">
	    <div class="card-panel blue-grey darken-1">
	      <span class="white-text"><%=fish.name%>
	      </span>
	    </div>
	  </div>
	</div>
	<% end %>
<h4>My following!</h4>
	<% @follow.each do |follow| %>
		<p><%=follow.following_user_id%></p>
	<% end %>
<!-- here to enter the cut out info -->

		<div>

		</div>
	</div>
</div>